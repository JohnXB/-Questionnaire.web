<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Css/style.css")
    @Styles.Render("~/Css/animate.css")
    @Styles.Render("~/Css/bootstrap.css")
    @Styles.Render("~/Css/icomoon.css")
    @Styles.Render("~/Css/magnific-popup.css")
</head>
<body>
    <div id="page">
        @Html.Partial("~/Views/Home/_Header.cshtml")

        @RenderBody()

        @Html.Partial("~/Views/Home/_Footer.cshtml")
    </div>
       
    @Scripts.Render("~/Js/jquery.min.js")
    @Scripts.Render("~/Js/jquery.easing.1.3.js")
    @Scripts.Render("~/Js/bootstrap.min.js")
    @Scripts.Render("~/Js/jquery.waypoints.min.js")
    @Scripts.Render("~/Js/jquery.countTo.js")
    @Scripts.Render("~/Js/jquery.magnific-popup.min.js")
    @Scripts.Render("~/Js/magnific-popup-options.js")
    @Scripts.Render("~/Js/jquery.stellar.min.js")
    @Scripts.Render("~/Js/main.js")
    @Scripts.Render("~/Js/modernizr-2.6.2.min.js")
    
    
</body>

</html>
<div id="loginModel" style="display:none">
    <div class="layui-form-item">
        <label class="layui-form-label">用户名</label>
        <div class="layui-input-block">
            <input type="text" id="account" name="account" required lay-verify="required" placeholder="请输入用户名" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item">
        <label class="layui-form-label">密&nbsp;码</label>
        <div class="layui-input-block">
            <input type="password" id="password" name="password" required lay-verify="required" placeholder="请输入密码" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div style="text-align:center;margin-bottom:10px;">
        <button class="layui-btn layui-btn-normal" onclick="Login_Ajax()">确定</button>
        <button class="layui-btn layui-btn-primary" onclick="cancel()">取消</button>
    </div>

</div>